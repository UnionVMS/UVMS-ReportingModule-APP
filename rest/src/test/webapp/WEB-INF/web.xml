<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0"
		 xmlns="http://java.sun.com/xml/ns/javaee">

	<filter>
		<filter-name>TempSecurityFilter</filter-name>
		<filter-class>eu.europa.ec.fisheries.uvms.reporting.rest.filter.TempSecurityFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>TempSecurityFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>

	<context-param>
		<param-name>usmApplication</param-name>
		<param-value>reporting</param-value>
	</context-param>
</web-app>